fn main() {
    let fahrenheit: f64 = 100.0;
    let celsius: f64 = (fahrenheit - 32.0) * 5.0 / 9.0;

    println!("{} degree fahrenheit is equal to {} degree celsius", fahrenheit, celsius);
}
